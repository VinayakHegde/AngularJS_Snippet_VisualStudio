<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets
    xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>AngularJS Constant</Title>
      <Author>Vinayak Hegde</Author>
      <Description>AngularJS Constant</Description>
      <Shortcut>ngconst</Shortcut>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>moduleName</ID>
          <ToolTip>Name of module</ToolTip>
          <Default>app</Default>
        </Literal>
        <Literal>
          <ID>constantName</ID>
          <ToolTip>Name of constant</ToolTip>
          <Default>MY_CONSTANT</Default>
        </Literal>
        <Literal>
          <ID>constantValue</ID>
          <ToolTip>Value of constant</ToolTip>
          <Default>true</Default>
        </Literal>
      </Declarations>
      <Code Language="JavaScript">
        <![CDATA[(function () {
    'use strict';

    angular
        .module('$moduleName$')
        .constant('$constantName$', $constantValue$)

})();
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>